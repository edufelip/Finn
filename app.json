{
  "expo": {
    "name": "Finn",
    "slug": "finn",
    "version": "2.0.0",
    "orientation": "portrait",
    "scheme": ["finn", "finn-dev"],
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#44A2D6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.edufelip.finn"
    },
    "android": {
      "package": "com.edufelip.finn",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#44A2D6"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-sqlite",
      "expo-notifications",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow access to your photo library to select images.",
          "cameraPermission": "Allow access to your camera to take photos."
        }
      ]
    ]
  }
}

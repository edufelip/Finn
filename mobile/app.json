{
  "expo": {
    "name": "Finn",
    "slug": "finn",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "finn",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#44A2D6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.edufelip.finn"
    },
    "android": {
      "package": "com.edufelip.finn",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#44A2D6"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-sqlite",
      "expo-notifications"
    ]
  }
}
